FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     simpleFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         5000;

deltaT          1;

writeControl    timeStep;

writeInterval   200;

purgeWrite      0;

writeFormat     ascii;

writePrecision  10;

writeCompression on;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

graphFormat     gnuplot;

functions
{
	pt
	{
		type         pressure;
		libs         ("libfieldFunctionObjects.so");
		writeControl writeTime;
		log          false;
        mode         totalCoeff;
        //mode         staticCoeff;
		pInf         0;
		UInf         (72 0 0);
        rho          rhoInf;
		rhoInf       1;
	}
	yPlus
	{
		type         yPlus;
		libs         ("libfieldFunctionObjects.so");
		writeControl writeTime;
		log          false;
	}
    UComponents
    {
        type            components;
        libs            ("libfieldFunctionObjects.so");
        field           U;
        executeInterval 1;
        writeControl    none;
    }
    solverInfo
    {
        type            solverInfo;
        libs            ("libutilityFunctionObjects.so");
        fields          (U p nuTilda);
        writeResidualFields false;
    }
    #includeFunc UxOut;
    #includeFunc UyOut;
    #includeFunc ptInlet;
    #includeFunc ptOutlet;

}

// ************************************************************************* //
