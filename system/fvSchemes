FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         steadyState;
}

gradSchemes
{
    default         Gauss linear;
	gradUConv		cellLimited Gauss linear 1;
}

divSchemes
{
    default                        Gauss linear;
    div(phi,U)                     Gauss upwind ;  // Gauss upwind;
    div(phi,nuTilda)               Gauss upwind ;  // Gauss linearUpwind gradUConv;
}

laplacianSchemes
{
    default         Gauss linear corrected;
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         corrected;
}

fluxRequired
{
    default         no;
    p               ;
}

wallDist
{
    method meshWave;
}


