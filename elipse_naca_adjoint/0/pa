FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      pa;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 2 -2 0 0 0 0 ];

internalField   uniform 0;

boundaryField
{
    walls
    {
        type            zeroGradient;
    }
    naca0012
    {
        type            zeroGradient;
    }
    elipse
    {
        type            zeroGradient;
    }
    outlet
    {
        type            adjointOutletPressure;
        value           uniform 0;
    }
    inlet
    {
        type 			zeroGradient;
    }
    front_n
    {
        type            empty;
    }
    back_n
    {
        type            empty;
    }
}


